{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["../*"]
    },
    "outDir": "dist",
    "resolveJsonModule": true
  },
  "include": ["src", "../shared"],
  "exclude": ["node_modules", "dist",  "src/runMintFlow.ts",]
}